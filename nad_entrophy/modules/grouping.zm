    lo_150_b, // low entropy interstimulus interval 150 ms language b
    lo_75_a,  // low entropy interstimulus interval 75 ms language a
    lo_75_b,  // low entropy interstimulus interval 75 ms language a
    hi_150_a, // high entropy interstimulus interval 150 ms language a
    hi_150_b, // high entropy interstimulus interval 150 ms language b
    hi_75_a,  // high entropy interstimulus interval 75 ms language a
    hi_75_b,  // high entropy interstimulus interval 75 ms language a
}
 
 
